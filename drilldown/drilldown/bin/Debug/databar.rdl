    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@state">
            <Value>=Parameters!state.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT TOP (20) MAX(Fact.Sale.Profit) AS profit, City_1.City
FROM     Dimension.City AS City_1 INNER JOIN
                  Fact.Sale ON City_1.[City Key] = Fact.Sale.[City Key]

GROUP BY City_1.City</CommandText>
      </Query>
      <Fields>
        <Field Name="profit">
          <DataField>profit</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="City">
          <DataField>City</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT top(20) [State Province]
FROM     Dimension.City</CommandText>
