      <Query>
        <DataSourceName>chart</DataSourceName>
        <CommandText>select distinct top(5) max( profit) as profit,
  city from WideWorldImportersDW.fact.sale as s 
inner join Dimension.city as c on s.[city key]=c.[city key] group by city</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
