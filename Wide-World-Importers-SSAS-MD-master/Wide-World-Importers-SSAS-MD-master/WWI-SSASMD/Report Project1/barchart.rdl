      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT TOP (5) MAX(Fact.Sale.Profit) AS profit, Fact.Sale.[Sale Key], City_1.City
FROM     Dimension.City AS City_1 INNER JOIN
                  Fact.Sale ON City_1.[City Key] = Fact.Sale.[City Key] AND City_1.[City Key] = Fact.Sale.[City Key] AND City_1.[City Key] = Fact.Sale.[City Key] AND City_1.[City Key] = Fact.Sale.[City Key]
GROUP BY City_1.City, Fact.Sale.[Sale Key]SELECT TOP (5) MAX(Fact.Sale.Profit) AS profit, Fact.Sale.[Sale Key], City_1.City
FROM     Dimension.City AS City_1 INNER JOIN
                  Fact.Sale ON City_1.[City Key] = Fact.Sale.[City Key] AND City_1.[City Key] = Fact.Sale.[City Key] AND City_1.[City Key] = Fact.Sale.[City Key] AND City_1.[City Key] = Fact.Sale.[City Key]
GROUP BY City_1.City, Fact.Sale.[Sale Key]</CommandText>
      </Query>
