  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT DISTINCT TOP (5) MAX(Profit) AS profit, [Sale Key]
FROM     Fact.Sale
GROUP BY [Sale Key]</CommandText>
      </Query>
      <Fields>
        <Field Name="Profit">
          <DataField>profit</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Sale_Key">
          <DataField>Sale Key</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!Profit.Value</FilterExpression>
          <Operator>TopN</Operator>
          <FilterValues>
            <FilterValue>5</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
  </DataSets>
