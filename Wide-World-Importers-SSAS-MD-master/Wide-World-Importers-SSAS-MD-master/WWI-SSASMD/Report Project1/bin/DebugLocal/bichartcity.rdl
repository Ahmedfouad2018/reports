  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource2</DataSourceName>
        <CommandText>SELECT DISTINCT TOP (5) Fact.Sale.Profit, City_1.[City Key], City_1.City
FROM     Dimension.City AS City_1 INNER JOIN
                  Fact.Sale ON City_1.[City Key] = Fact.Sale.[City Key] AND City_1.[City Key] = Fact.Sale.[City Key] AND City_1.[City Key] = Fact.Sale.[City Key] AND City_1.[City Key] = Fact.Sale.[City Key]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="City_Key">
          <DataField>City Key</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Profit">
          <DataField>Profit</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="City">
          <DataField>City</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!City_Key.Value</FilterExpression>
          <Operator>TopN</Operator>
          <FilterValues>
            <FilterValue DataType="Integer">5</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
  </DataSets>
