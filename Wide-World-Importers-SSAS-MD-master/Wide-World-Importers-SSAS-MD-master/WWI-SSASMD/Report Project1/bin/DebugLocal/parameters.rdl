  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>chartandtable</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BuyingGroup">
            <Value>=Parameters!BuyingGroup.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  fs.[Delivery Date Key]
  ,fs.[WWI Invoice ID]
  ,fs.[Description]
  ,fs.[Total Excluding Tax]
  ,dc.[Customer]
  ,dc.[Buying Group]
  ,dd.[Calendar Year]
  FROM
  Fact.Sale fs
  INNER JOIN  Dimension.[Date] dd
ON fs.[Delivery Date Key] = dd.[Date]
INNER JOIN Dimension.Customer dc
 ON fs.[Customer Key] = dc.[Customer Key] 
 INNER JOIN Dimension.[Employee]
    ON fs.[Salesperson Key] = Dimension.[Employee].[Employee Key]

where dc.[Buying Group] IN (@BuyingGroup) 
AND dd.[Calendar Year] IN (@Year)</CommandText>
      </Query>
      <Fields>
        <Field Name="Customer">
          <DataField>Customer</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Buying_Group">
          <DataField>Buying Group</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="WWI_Invoice_ID">
          <DataField>WWI Invoice ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total_Excluding_Tax">
          <DataField>Total Excluding Tax</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Calendar_Year">
          <DataField>Calendar Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Delivery_Date_Key">
          <DataField>Delivery Date Key</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="BuyingGroup">
      <Query>
        <DataSourceName>chartandtable</DataSourceName>
        <CommandText>SELECT [Buying Group]
FROM     Dimension.Customer
ORDER BY [Buying Group]</CommandText>
      </Query>
      <Fields>
        <Field Name="Buying_Group">
          <DataField>Buying Group</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Year">
      <Query>
        <DataSourceName>chartandtable</DataSourceName>
        <CommandText>SELECT [Calendar Year]
FROM     Dimension.Date
ORDER BY [Calendar Year]</CommandText>
      </Query>
      <Fields>
        <Field Name="Calendar_Year">
          <DataField>Calendar Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
