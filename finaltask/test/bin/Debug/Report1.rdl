          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="totalexculdingsale">
          <DataField>totalexculdingsale</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Profit">
          <DataField>Profit</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
