  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>datac</DataSourceName>
        <CommandText>SELECT distinct top(20) City_1.City, Dimension.Date.[Calendar Year], Fact.[Order].Description, Fact.[Order].[Total Excluding Tax], Fact.Sale.[Total Excluding Tax] AS totalexculdingsale, Fact.Sale.Profit
FROM     Dimension.City AS City_1 INNER JOIN
                  Fact.[Order] ON City_1.[City Key] = Fact.[Order].[City Key] INNER JOIN
                  Fact.Sale ON City_1.[City Key] = Fact.Sale.[City Key] CROSS JOIN
                  Dimension.Date</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="City">
          <DataField>City</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
