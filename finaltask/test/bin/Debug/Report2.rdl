  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT distinct top(20)Fact.Sale.[Total Excluding Tax], Fact.Sale.Profit, Fact.[Order].[Total Excluding Tax] AS Expr1, City_1.City
FROM     Dimension.City AS City_1 INNER JOIN
                  Fact.[Order] ON City_1.[City Key] = Fact.[Order].[City Key] AND City_1.[City Key] = Fact.[Order].[City Key] INNER JOIN
                  Fact.Sale ON City_1.[City Key] = Fact.Sale.[City Key] AND City_1.[City Key] = Fact.Sale.[City Key]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
