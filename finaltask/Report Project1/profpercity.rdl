  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT TOP (5) Sum(Fact.Sale.Profit) AS profit, City_1.City, Fact.Sale.color, City_1.[City Key]
FROM     Dimension.City AS City_1 INNER JOIN
                  Fact.Sale ON City_1.[City Key] = Fact.Sale.[City Key] AND City_1.[City Key] = Fact.Sale.[City Key]
GROUP BY City_1.City, Fact.Sale.color, City_1.[City Key]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
