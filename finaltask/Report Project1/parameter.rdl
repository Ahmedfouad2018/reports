  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT tblStudio.StudioName, tblFilm.FilmRunTimeMinutes, tblFilm.FilmBudgetDollars, tblFilm.FilmBoxOfficeDollars, tblFilm.FilmOscarWins, tblFilm.FilmOscarNominations, tblCertificate.Certificate, tblCountry.CountryName, 
                  tblDirector.DirectorName
FROM     tblFilm INNER JOIN
                  tblCertificate ON tblFilm.FilmCertificateID = tblCertificate.CertificateID INNER JOIN
                  tblCountry ON tblFilm.FilmCountryID = tblCountry.CountryID INNER JOIN
                  tblStudio ON tblFilm.FilmStudioID = tblStudio.StudioID INNER JOIN
                  tblDirector ON tblFilm.FilmDirectorID = tblDirector.DirectorID</CommandText>
      </Query>
