  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT     top(5)      max( Fact.Sale.Profit )as profit
,color,City_1.City 
FROM            Dimension.City AS City_1 INNER JOIN
                         Fact.Sale ON City_1.[City Key] =  Fact.Sale.[City Key]
group by color,City_1.City</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
