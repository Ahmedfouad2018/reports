  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT      top(5) max(Fact.Sale.Profit) as profit, Dimension.Employee.Employee,color
FROM            Dimension.Employee INNER JOIN
                         Fact.Sale ON Dimension.Employee.[Employee Key] = Fact.Sale.[Salesperson Key]
group by Dimension.Employee.Employee,color</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
