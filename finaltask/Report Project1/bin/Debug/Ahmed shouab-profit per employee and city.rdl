  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Data</DataSourceName>
        <CommandText>SELECT      top(400)  City_1.City, Dimension.Employee.Employee,max(Fact.Sale.Profit) as profit
FROM            Dimension.City AS City_1 INNER JOIN
                         Fact.Sale ON City_1.[City Key] = Fact.Sale.[City Key] INNER JOIN
                         Dimension.Employee ON Fact.Sale.[Salesperson Key] = Dimension.Employee.[Employee Key]
group by City_1.City,Dimension.Employee.Employee
order by profit</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="City">
          <DataField>City</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Employee">
          <DataField>Employee</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Profit">
          <DataField>profit</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
