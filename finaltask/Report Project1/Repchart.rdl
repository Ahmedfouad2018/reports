  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT  top(5) Fact.Sale.[Total Excluding Tax], max(Fact.Sale.Profit)as profit, Dimension.Employee.Employee
FROM     Fact.Sale INNER JOIN
                  Dimension.Employee ON Fact.Sale.[Salesperson Key] = Dimension.Employee.[Employee Key] group by Fact.Sale.[Total Excluding Tax],Dimension.Employee.Employee</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Total_Excluding_Tax">
          <DataField>Total Excluding Tax</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Profit">
          <DataField>profit</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Employee">
          <DataField>Employee</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!Employee.Value</FilterExpression>
          <Operator>TopN</Operator>
          <FilterValues>
            <FilterValue>5</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
  </DataSets>
